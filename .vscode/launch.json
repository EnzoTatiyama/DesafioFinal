{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run CRUD_Plataforma-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/crud-plataforma-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/desafiol4eapp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/app",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/CRUD_Plataforma/app\"}"
      },
      "envFile": "${workspaceFolder}/app/.env1",
      "preLaunchTask": "Build CRUD_Plataforma"
    },
    {
      "name": "Run L4E-Desafio-SAPUI5-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/crud-plataforma-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/desafiol4eapp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/app",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/L4E-Desafio-SAPUI5/app\"}"
      },
      "envFile": "${workspaceFolder}/app/.env2",
      "preLaunchTask": "Build L4E-Desafio-SAPUI5"
    }
  ]
}